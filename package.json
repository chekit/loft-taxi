  "scripts": {
    "start:win": "cross-env SASS_PATH=./node_modules;./src react-scripts start",
    "start:mac": "cross-env SASS_PATH=node_modules:src react-scripts start",
    "build:win": "cross-env SASS_PATH=./node_modules;./src react-scripts build",
    "build:mac": "cross-env SASS_PATH=node_modules:src react-scripts build",
    "test:win": "cross-env SASS_PATH=./node_modules;./src react-scripts test",
